  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "property-validator": "^0.8.0",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "autod": "^3.0.0",
